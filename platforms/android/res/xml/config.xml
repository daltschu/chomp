<?xml version='1.0' encoding='utf-8'?>
<widget id="com.daltschu.chomp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.admob.AdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Chomp</name>
    <description>
        The game chomp
    </description>
    <author email="danaltschul@yahoo.com">
        Daniel Altschuler
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="res/iconchoc128.png" />
    <preference name="Orientation" value="portrait" />
</widget>
